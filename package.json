{
  "name": "gamergrep",
  "version": "0.1.1",
  "description": "Your Games. Your stats.",
  "author": "Michael Mazzone",
  "private": true,
  "keywords": [],
  "homepage": "https://github.com/GitMazzone/gamergrep#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GitMazzone/gamergrep.git"
  },
  "bugs": {
    "url": "https://github.com/GitMazzone/gamergrep/issues"
  },
  "main": "app/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "watch": "webpack --config build/webpack.app.config.js --watch",
    "start": "electron .",
    "test": "jest",
    "e2e": "yarn run cypress open",
    "commit": "cz",
    "dist": "webpack --config build/webpack.app.config.js --env=production && electron-builder"
  },
  "build": {
    "appId": "com.electron.gamergrep",
    "productName": "GamerGrep",
    "copyright": "Copyright Â© 2021 Michael Mazzone",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.games",
      "icon": "build/assets/icon-mac.png"
    },
    "win": {
      "icon": "build/assets/icon-windows.png"
    },
    "publish": null
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "electron-reload": "^1.5.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.2.9",
    "autoprefixer": "^9",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "commitizen": "4.2.4",
    "concurrently": "^6.2.0",
    "css-loader": "^5.2.4",
    "cypress": "^7.3.0",
    "cz-conventional-changelog": "3.3.0",
    "electron": "^12.0.9",
    "electron-builder": "^22.10.5",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "postcss": "^7",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^2.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  }
}
